<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block name="ui.component.example" template="Magento_Ui::ui-component-example.phtml" before="-">
                <arguments>
                    <argument name="jsLayout" xsi:type="array">
                        <item name="components" xsi:type="array">
                            <item name="ui-component-example" xsi:type="array">
                                <item name="component" xsi:type="string">Magento_Ui/js/view/ui-component-example</item>
                                <item name="config" xsi:type="array">
                                    <item name="template" xsi:type="string">Magento_Ui/ui-component-example</item>
                                </item>
                            </item>
                        </item>
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
